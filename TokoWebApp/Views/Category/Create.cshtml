@model TokoWebApp.Models.CategoryModel
@{
    ViewData["Title"] = "Add Product Type";
}
<h1>Add new Product Type</h1>
<form method="post" asp-controller="Category" asp-action="Create" class="mt-5">
    <input type="hidden" asp-for="CategoryId" />
    <div class="mb-3">
        <label class="form-label">Name</label>
        <input type="text" class="form-control" asp-for="CategoryName" />
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
